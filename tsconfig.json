{
	"compilerOptions": {
		"strict": true,
		"noEmit": true,
		"allowJs": true,
		"moduleResolution": "nodenext",
		"module": "NodeNext",
		"skipLibCheck": true,
		"noUnusedLocals": true,
		"noUnusedParameters": true,
		"jsx": "react-jsx",
		"target": "ES6",
		"lib": ["DOM", "DOM.Iterable", "ES2019"]
	}
}
